# Copy to .env and adjust values for local development
TOKBOT_ENV=development
TOKBOT_DEFAULT_AGENT=echo
# Comma-separated module paths exposing a build_agent() factory.
# Defaults include tokbot.agents.echo, uppercase, planner, builder, auditor.
TOKBOT_AGENT_MODULES=
# Directory for workflow transcript exports (default: artifacts/transcripts)
TOKBOT_TRANSCRIPTS_DIR=
# Default repository for GitHub issue interactions (owner/repo)
TOKBOT_GITHUB_REPO=
